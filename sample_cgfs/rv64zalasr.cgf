sb.rl:
    config: 
      - check ISA:=regex(.*Zalasr.*)
    opcode: 
      sb.rl 0
    rs1: 
      <<: *all_regs_mx0
    rs2: 
      <<: *all_regs
    op_comb: 
        'rs1 != rs2': 0
    val_comb:
        'ea_align == 0': 0
        'ea_align == 1': 0
        'ea_align == 2': 0
        'ea_align == 3': 0
        <<: [  *base_rs2val_sgn]
        abstract_comb:
          <<: [*rs2val_walking]


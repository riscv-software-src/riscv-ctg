# cover group format file for Zalasr extension
lb.aq:
    config: 
      - check ISA:=regex(.*Zalasr.*)
    opcode: 
      lb.aq 0
    rs1: 
      <<: *all_regs_mx0
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *ifmt_op_comb
    val_comb:
      'ea_align == 0'
      'ea_align == 1'
      'ea_align == 2'
      'ea_align == 3'

lh.aq:
    config: 
      - check ISA:=regex(.*Zalasr.*)
    opcode: 
      lh.aq 0
    rs1: 
      <<: *all_regs_mx0
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *ifmt_op_comb
    val_comb:
      'ea_align == 0'
      'ea_align == 2'

